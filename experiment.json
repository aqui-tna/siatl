{
  "name": "alexandra-chron/siatl",
  "url": "https://github.com/aqui-tna/siatl",
  "python_version": "3.6",
  "requirements": {
    "req_file": "requirements.txt",
    "pip": [
      "torch"
    ],
    "others": [
      "tar"
    ]
  },
  "to_run": {
      "decision": true,
      "reason": ""
  },
  "entries": [
      {
        "cmd": "python models/sent_lm.py",
        "main": "training",
        "steps": [
            "data-preparation or training or testing or data-preprocessing or anything else"
        ],
        "args_lib": "argparse",
        "args": [
          {
              "names": [
                  "-i", 
                  "--input"
              ],
              "input": true,
              "dest": "input",
              "ex_has_default": true,
              "default": "lm_20m_word.yaml",
              "type": "str",
              "mandatory": false
          }
      ],
        "ins": [
            {
            "type": "dir",
            "location": "model_configs/lm_20m_word.yaml",
            "artifact": "lm_20m_word.yaml",
            "ex_from": "conf",
            "ex_in_repo": true
            }
        ],
        "outs": [
            {
                "type": "",
                "location": "",
                "artifact": "",
                "ex_from": "",
                "ex_in_repo": false
            }
        ]
        },
        {
          "cmd": "python models/run_clf.py",
          "main": "run_clf.py",
          "steps": [
              "Running to fine-tune"
          ],
          "args_lib": "argparse",
          "args": [
            {
                "names": [
                    "-i",
                    "--input"
                ],
                "input": true,
                "dest": "input",
                "ex_has_default": true,
                "default": "SCV2_aux_ft_gu.yaml",
                "type": "str",
                "mandatory": false
            },
            {
                "names": [
                    "-t",
                    "--transfer"
                ],
                "input": false,
                "dest": "transfer",
                "ex_has_default": true,
                "default": false,
                "type": "float",
                "mandatory": false
            },
            {
                "names": [
                    "-a",
                    "--aux_loss"
                ],
                "input": false,
                "dest": "aux_loss",
                "ex_has_default": true,
                "default": false,
                "type": "float",
                "mandatory": false
            }
        ],
          "ins": [
              {
              "type": "dir",
              "location": "model_configs/SCV2_aux_ft_gu.yaml",
              "artifact": "SCV2_aux_ft_gu.yaml",
              "ex_from": "conf",
              "ex_in_repo": true
              }
          ],
          "outs": [
              {
                  "type": "",
                  "location": "",
                  "artifact": "",
                  "ex_from": "",
                  "ex_in_repo": false
              }
          ]
          }
    ],
  "ex_metrics": {
    "metrics_files": [
        {
            "file_name": "",
            "metrics": [""]
        }
    ],
    "prints": [""],
    "plots": [""],
    "others": [
        {
            "name": "method name",
            "in_repo": "true if the file is in the repo, false if not",
            "metrics": ["loss","accuracy","f1"]
        }
    ]
  },
  "ex_hyperparams": {
    "from_cmd": ["--aux_loss","--transfer"],
    "config_files": [
        {
            "name": "model_configs/lm20m_70K",
            "in_repo": "true",
            "hyperparams": ["weight_decay","epochs","batch_size", "log_interval", "checkpoint_interval"]
        },
        {
          "name": "model_configs/scv2_aux_ft_gu",
          "in_repo": "true",
          "hyperparams": ["weight_decay","epochs","batch_size", "log_interval", "checkpoint_interval","exp_decay","unfreeze_rnn"]
      }
    ],
    "in_code": [],
    "others": [
        {
            "name": "sent_clf",
            "metrics": "loss, accuracy"
        }
    ]
  }
}